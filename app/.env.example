MONGODB_URI=mongodb://localhost/db
REDIS_URI=redis://localhost
KEK_VERSION=KEK_VERSION_1

# generate this value using `openssl rand -base64 32`
KEK_VERSION_1=

# generate this value using `openssl rand -base64 32`
AUTH_SECRET=

# Full domain for the proxy (e.g., proxy.example.com or proxy-cors.example.com)
PROXY_DOMAIN=proxy.localhost

# Set to true to disable new user signups (self-hosted only, ignored when CLOUD=true)
# Recommended: First signup, then set DISABLE_SIGNUP=true to prevent other users from registering
DISABLE_SIGNUP=false
