MONGODB_USER=proxy
MONGODB_PASSWORD=corsfix
REDIS_PASSWORD=corsfix
# generate this value using `openssl rand -base64 32`
KEK_VERSION_1=
# generate this value using `openssl rand -base64 32`
AUTH_SECRET=

# Full domain for the app (e.g., app.example.com or admin-cors.example.com)
APP_DOMAIN=app.localhost

# Full domain for the proxy (e.g., proxy.example.com or proxy-cors.example.com)
PROXY_DOMAIN=proxy.localhost

# Set to true to disable new user signups (self-hosted only, ignored when CLOUD=true)
# Recommended: First signup, then set DISABLE_SIGNUP=true to prevent other users from registering
DISABLE_SIGNUP=false

# Rate limit (requests per minute) for self-hosted instances (default: 180)
RPM=180

# Custom ports (optional, if 80/443 are already in use)
HTTP_PORT=80
HTTPS_PORT=443

# Custom TLS certificates (optional)
# Set paths to your cert and key files, then uncomment the tls lines in Caddyfile
TLS_CERT=
TLS_KEY=
